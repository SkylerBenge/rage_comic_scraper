<h1>Listing comics</h1>

<table>
  <tr>
    <th>Domain</th>
    <th>Score</th>
    <th>Nsfw</th>
    <th>Downs</th>
    <th>Reddit url</th>
    <th>Comic url</th>
    <th>Title</th>
    <th>Submitted at</th>
    <th>Ups</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @comics.each do |comic| %>
  <tr>
    <td><%= comic.domain %></td>
    <td><%= comic.score %></td>
    <td><%= comic.nsfw %></td>
    <td><%= comic.downs %></td>
    <td><%= comic.reddit_url %></td>
    <td><%= comic.comic_url %></td>
    <td><%= comic.title %></td>
    <td><%= comic.submitted_at %></td>
    <td><%= comic.ups %></td>
    <td><%= link_to 'Show', comic %></td>
    <td><%= link_to 'Edit', edit_comic_path(comic) %></td>
    <td><%= link_to 'Destroy', comic, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Comic', new_comic_path %>
